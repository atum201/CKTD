    </property>
    <property name="TrangThai">
      <column name="TrangThai" sql-type="int" not-null="false" />
    </property>
    <property name="ThoiGianCapNhat">
      <column name="ThoiGianCapNhat" sql-type="datetime" not-null="false" />
